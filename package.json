    "@types/jsdom": "^21.1.7",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.1",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "googleapis": "^160.0.0",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.542.0",
    "multer": "^2.0.2",
    "next": "15.5.9",
    "node-fetch": "^3.3.2",
